(drive truck1 e5 d5)
(pick-up truck1 d5 p2 capacity-3 capacity-4)
(drive truck1 d5 d4)
(drive truck1 d4 e4)
(pick-up truck1 e4 p1 capacity-2 capacity-3)
(drive truck1 e4 d4)
(drive truck1 d4 c4)
(drive truck1 c4 b4)
(drive truck1 b4 b3)
(drive truck1 b3 b2)
(drive truck1 b2 b1)
(drop truck1 b1 p1 capacity-2 capacity-3)
(drive truck1 b1 a1)
(drop truck1 a1 p2 capacity-3 capacity-4)
; cost = 1004 (general cost)
